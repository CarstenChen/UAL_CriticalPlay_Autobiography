    audio: {fileID: 8300000, guid: 58fe49badaf248d48853f4d022e363a1, type: 3}
    blockInput: 1
  - plotID: 3
    index: 2
    nextIndex: 3
    text: Come on, we should balance work and play.
    playerID: 0
    audio: {fileID: 8300000, guid: d64fd5967fc800e468de00b5775c3aa1, type: 3}
    blockInput: 1
  - plotID: 3
    index: 3
